---
layout: none
---
BEGIN:VCALENDAR
PRODID:-//Harris Lapiroff//Contra Calling Calendar//EN
VERSION:2.0
METHOD:PUBLISH
{% for dance in site.data.contra.caller.upcoming %}BEGIN:VEVENT
UID:{{ dance.date | date: "%Y%m%dT%H%M%SZ" }}-contra-dance@harrislapiroff.com
ORGANIZER;CN="Harris Lapiroff":MAILTO:harris@harrislapiroff.com
LOCATION:{{ dance.street }}, {{ dance.city }}
SUMMARY:{{ dance.name }}
DESCRIPTION:Caller: Harris Lapiroff\nBand: {{ dance.band }}\n{{ dance.description | replace: "
", '\\n' }}
CLASS:PUBLIC
DTSTART;TZID={{ dance.tzid }}:{{ dance.date | date:"%Y%m%dT%H%M%SZ" }}
DTEND;TZID={{ dance.tzid }}:{{ dance.date_end | date:"%Y%m%dT%H%M%SZ" }}
DTSTAMP:{{ 'now' | date:"%Y%m%dT%H%M%SZ" }}
END:VEVENT
{% endfor %}END:VCALENDAR